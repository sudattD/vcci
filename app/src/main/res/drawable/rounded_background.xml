                android:color="#909090" />
            <!--<padding
                android:bottom="0.3dp"
                android:left="0dp"
                android:right="0.3dp"
                android:top="0dp" />-->
                        <corners android:radius="5dp" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke
                android:width="1dp"
                android:color="#A9A9A9" />

            <corners android:radius="5dp" />
